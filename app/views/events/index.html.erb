<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Event date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.event_date %></td>
        <td><%= link_to "Show", event %></td>
             </tr>
    <% end %>
  </tbody>
</table>

<br>

 <%= link_to "New Event", new_event_path %>
